{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
      "react": {
        "version": "18.1"
      }
    },
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow": ["_id"]
        }
      ]
    }
}

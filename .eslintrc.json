{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow": ["_id"]
        }
      ]
    }
}
